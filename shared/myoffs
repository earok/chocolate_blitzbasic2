opendos   macro
   lea   dosname(pc),a1
   move.l   4,a6
   jsr   oldopenlibrary(a6)
   move.l   d0,dos
   bra   dos+4
dosname   dc.b   'dos.library',0
   even
dos   dc.l   0
   endm

calldos	macro
	move.l	dos(pc),a6
	jsr	\1(a6)
	endm

openint   macro
   lea   intname(pc),a1
   move.l   4,a6
   jsr   oldopenlibrary(a6)
   move.l   d0,int
   bra   int+4
intname   dc.b   'intuition.library',0
   even
int   dc.l   0
   endm

callint	macro
	move.l	int(pc),a6
	jsr	\1(a6)
	endm

opengr   macro
   lea   grname(pc),a1
   move.l   4,a6
   jsr   oldopenlibrary(a6)
   move.l   d0,gr
   bra   gr+4
grname   dc.b   'graphics.library',0
   even
gr   dc.l   0
   endm

callgr	macro
	move.l	gr(pc),a6
	jsr	\1(a6)
	endm

closedos   macro
   move.l   dos(pc),a1
   move.l   4,a6
   jsr   closelibrary(a6)
   endm

closeint   macro
   move.l   int(pc),a1
   move.l   4,a6
   jsr   closelibrary(a6)
   endm

closegr   macro
   move.l   gr(pc),a1
   move.l   4,a6
   jsr   closelibrary(a6)
   endm


